{
  "name": "refactorthis",
  "displayName": "RefactorThis",
  "description": "Common TS refactoring operations",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onCommand:refactorthis.to-arrow-syntax"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "refactorthis.to-arrow-syntax",
        "title": "[RThis] Convert to arrow syntax (=>)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "chai": "^4.2.0",
    "glob": "^7.1.5",
    "mocha": "^7.1.1",
    "prettier": "^1.19.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4",
    "vscode-test": "^1.2.2"
  }
}

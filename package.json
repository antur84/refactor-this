{
  "name": "refactorthis",
  "displayName": "RefactorThis",
  "description": "Common TS refactoring operations",
  "version": "1.0.0",
  "publisher": "AndreasTurku",
  "repository": {
    "url": "https://github.com/antur84/refactor-this"
  },
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript"
  ],
  "main": "./dist/extension",
  "contributes": {
    "commands": [
      {
        "command": "refactorthis.to-arrow-syntax",
        "title": "[RThis] Convert to arrow syntax (=>)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch",
    "test-compile": "tsc -p ./"
  },
  "prettier": {
    "singleQuote": true
  },
  "devDependencies": {
    "@types/chai": "^5.0.0",
    "@types/chai-spies": "^1.0.6",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.8.1",
    "@types/vscode": "^1.94.0",
    "chai": "^5.1.2",
    "chai-spies": "^1.1.0",
    "glob": "^11.0.0",
    "mocha": "^10.7.3",
    "prettier": "^3.3.3",
    "ts-loader": "^9.5.1",
    "tslint": "^6.1.3",
    "typescript": "^5.6.3",
    "vscode-test": "^1.2.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  }
}
